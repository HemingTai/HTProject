# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'
source 'https://cdn.cocoapods.org/'
install! 'cocoapods', generate_multiple_pod_projects: true

target 'HTProject_Swift' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'Alamofire', :project_name => 'Networking'
  pod 'SnapKit', :project_name => 'Layout'
  pod 'Socket.IO-Client-Swift', :project_name => 'Socket'
  
end

target 'HTProject_OC' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  pod 'ReactiveObjC', :project_name => 'Tool'
  pod 'AFNetworking', :project_name => 'Networking'
  pod 'SDWebImage', :project_name => 'Tool'
  pod 'YYKit', :project_name => 'Tool'
  pod 'Masonry', :project_name => 'Layout'
  pod 'CTMediator', :project_name => 'Tool'
  pod 'Socket.IO-Client-Swift', :project_name => 'Socket'
  pod 'MJRefresh', :project_name => 'Tool'
  pod 'IQKeyboardManager', :project_name => 'Tool'
  
end
